<?xml version="1.0" encoding="utf-8" ?>
<c2uistate>
    <!--This is a UI state file - its content describes the user interface settings when the project was last open.
It is entirely optional and the project will load without it.  If you are placing the project under source control,
you probably do not want to add any .uistate.xml files to the repository.-->
    <configurations>
        <active>
            <configuration name="HTML5" />
        </active>
        <display name="HTML5" />
    </configurations>
    <open-tabs>
        <layout order="0">Game</layout>
        <layout order="10">Title</layout>
        <event-sheet order="1">Main Game</event-sheet>
        <event-sheet order="2">Globals</event-sheet>
        <event-sheet order="3">Mushrooms</event-sheet>
        <event-sheet order="4">Player</event-sheet>
        <event-sheet order="5">Centipede</event-sheet>
        <event-sheet order="6">TODO</event-sheet>
        <event-sheet order="7">State</event-sheet>
        <event-sheet order="8">Spider, Scorpion and Flea</event-sheet>
        <event-sheet active="1" order="9">Title Sheet</event-sheet>
    </open-tabs>
    <properties>
        <property expanded="1" name="About" />
        <property expanded="1" name="Project settings" />
        <property expanded="0" name="Window Size" />
        <property expanded="1" name="Configuration Settings" />
    </properties>
    <families-expanded root="1">
        <family expanded="1" name="Colourable" />
        <family expanded="1" name="Animals" />
    </families-expanded>
    <folders-expanded>
        <folder expanded="1" path="object-folder/" />
        <folder expanded="1" path="layout-folder/" />
        <folder expanded="1" path="event-folder/" />
        <folder expanded="1" path="file-folder/" />
        <folder expanded="1" path="sound-folder/" />
        <folder expanded="1" path="music-folder/" />
        <folder expanded="0" path="file-folder/Icons/" />
        <folder expanded="1" path="animation-folder/Mushroom/" />
        <folder expanded="1" path="animation-folder/Player/" />
        <folder expanded="1" path="animation-folder/Missile/" />
        <folder expanded="1" path="animation-folder/Segment/" />
        <folder expanded="1" path="animation-folder/LivesMarker/" />
        <folder expanded="1" path="animation-folder/ScoreMushroom/" />
        <folder expanded="1" path="animation-folder/Spider/" />
        <folder expanded="1" path="animation-folder/Flea/" />
        <folder expanded="1" path="animation-folder/Scorpion/" />
        <folder expanded="1" path="animation-folder/CircleLetter/" />
        <folder expanded="1" path="animation-folder/TitleMushroom/" />
        <folder expanded="1" path="animation-folder/ColourBar/" />
    </folders-expanded>
